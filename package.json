{
  "name": "brewdown",
  "version": "0.1.0",
  "description": "A markdown parser and stylesheet specifically for D&D homebrew.",
  "main": "./src/js/index.js",
  "scripts": {
    "build": "npm run clean && npm run lint && node ./build/index.js",
    "clean": "rimraf ./dist",
    "lint": "standard --env jasmine --fix --verbose | snazzy",
    "test": "karma start --single-run",
    "test_watch": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trwolfe13/brewdown.git"
  },
  "keywords": [
    "homebrew",
    "markdown",
    "D&D",
    "dungeons-and-dragons",
    "rpg",
    "markdown-it",
    "markdown-it-plugin"
  ],
  "author": "Tom Wolfe <trwolfe13@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/trwolfe13/brewdown/issues"
  },
  "homepage": "https://github.com/trwolfe13/brewdown#readme",
  "devDependencies": {
    "fs-extra": "^4.0.0",
    "jasmine-core": "^2.7.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "klaw": "^2.0.0",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "markdown-it": "^8.3.1",
    "markdown-it-container": "^2.0.0"
  }
}
